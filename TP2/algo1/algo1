def algo1(a, b, c, d):
   """permet de trouver le plus petit nombre entre 4 nombres
    Args:
    a (int): le premier nombre entrer
    b (int): le deuxime nombre entrer
    c (int): le troisieme nombre entrer
    d (int): le quatrieme nombre entrer

Returns:
    int: le plus petit nombre des 4
  
"""

    if a < b
        res = a
    else
        res = b
    if c < res
        res = c
    if d < res
        res = d
    return res
    